{
    "name": "agricultural-supply-chain-chaincode",
    "version": "1.0.0",
    "description": "Agricultural supply chain chaincode for batch tracking, certification, and transfers",
    "main": "index.js",
    "engines": {
        "node": ">=12",
        "npm": ">=5"
    },
    "scripts": {
        "start": "node index.js",
        "test": "nyc mocha test --recursive"
    },
    "dependencies": {
        "fabric-contract-api": "^2.1.0",
        "fabric-shim": "^2.1.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^5.6.0",
        "mocha": "^6.2.0",
        "nyc": "^14.1.1",
        "sinon": "^6.3.5",
        "sinon-chai": "^3.3.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}